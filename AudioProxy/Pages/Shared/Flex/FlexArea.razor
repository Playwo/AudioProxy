@namespace AudioProxy.Pages

<div id="@Id" class="@($"{(Center ? "center" : "")} {Class}")">
    @ChildContent
</div>

@code{
    [Parameter]
    public RenderFragment ChildContent { get; set; }

    [Parameter]
    public bool Center { get; set; }

    [Parameter]
    public string Id { get; set; }

    [Parameter]
    public string Class { get; set; }
}